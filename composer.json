{
    "name": "brilliance/algolia-sync",
    "description": "Syncing elements with Algolia using their API",
    "type": "craft-plugin",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "algolia sync"
    ],
    "support": {
        "docs": "https://github.com/brilliancenw/craft-algolia-sync",
        "issues": "https://github.com/brilliancenw/craft-algolia-sync/issues"
    },
    "license": "Craft",
    "authors": [
        {
            "name": "Brilliance",
            "homepage": "https://www.brilliancenw.com/about"
        }
    ],
    "require": {
        "craftcms/cms": ">=3.4.18 <4",
        "algolia/algoliasearch-client-php": "^3"
    },
    "autoload": {
        "psr-4": {
          "brilliance\\algoliasync\\": "src/"
        }
    },
    "extra": {
        "name": "Algolia Sync",
        "handle": "algolia-sync",
        "hasCpSettings": true,
        "hasCpSection": false,
        "changelogUrl": "???",
        "components": {
            "algoliaSyncService": "brilliance\\algoliasync\\services\\AlgoliaSyncService"
        },
        "class": "brilliance\\algoliasync\\AlgoliaSync"
    }
}
